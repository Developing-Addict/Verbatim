{
	"name": "Verbatim",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerStorage": {
			"$className": "ServerStorage",
			"Verbatim": {
				"$path": "out/verbatim"
			}
		},
		"ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "rbxts_include": {
                "$path": "include",
                "node_modules": {
                    "$path": "node_modules/@rbxts"
                }
            }
        }
	}
}
